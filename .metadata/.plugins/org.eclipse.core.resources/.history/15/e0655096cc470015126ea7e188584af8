package expackage;

class IllegalAgeException extends Exception{
	
}
class Vote{
	public void register(int age) throws IllegalAgeException{
		if(age < 18){
			throw new IllegalAgeException();
		}
		else{
			System.out.println("You are eligible to register for vote");
		}
	}
} 

