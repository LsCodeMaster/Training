    xhr.onreadystatechange = function() {
        if (xhr.readyState == 4) {
            var data = xhr.responseText;
            alert(data);
            document.getElementById("insert").innerHTML = xmlHttp.responseText;
        }
    }
    xhr.open('GET', '../../../CounterServlet', true);
    xhr.send(null);
</script>
