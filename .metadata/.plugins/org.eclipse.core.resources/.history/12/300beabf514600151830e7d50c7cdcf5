 */
package Science;

import math.Multiplication;

/**
 * @author ilisze
 *
 */
public class Physics extends Multiplication{
	final double GRAVITY = 9.81;
	public double potentialEnergy(){
		Multiplication arith = new Multiplication();
		int height = arith.num2;
		int mass = arith.num3;
		return GRAVITY*height*mass;
	}
}
